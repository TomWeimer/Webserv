Server content:
	name: webserv
	port: 0, host: 4242, socket: 0.0.0.0:4242
	root: ./www/html
	allowed_methods: GET
	index: index.html, index2.html

	error_pages: 
	404 -> ./www/html/error/error.html
	405 -> ./www/html/error/error.html
	500 -> ./www/html/error/error2.html
	body limit: 4096

	location = /board
	{
		root: ./www/html
		allowed_methods: GET
		index: 
		error_pages: 
		body limit: 0
	}

	location = /board/content
	{
		root: ./www/html/contents
		allowed_methods: GET, POST, DELETE
		index: board.html
		error_pages: 
		body limit: 0
	}
